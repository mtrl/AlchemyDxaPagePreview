@model LinkList<Link>
@if (Model.Links!=null && Model.Links.Count>0)
{
    <ul class="nav navbar-nav utility-nav" @Markup.Entity(Model)>
        @{ int index = 0; }   
        @foreach(var link in Model.Links)
        {
            <li @Markup.Property(Model, "Links", index)>
                <a href="@link.Url" title="@link.AlternateText">@link.LinkText</a>
            </li>
            index++;
        }
    </ul>
}